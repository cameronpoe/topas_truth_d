#this is a helper file composed of only volumetric sources over each of the XCAT phantom materials/tissues

#defines a volumetric source over the Patient geometry
s:So/XCAT_yellow_bone_marrow_activitySource/Type            = "Volumetric"
s:So/XCAT_yellow_bone_marrow_activitySource/Component       = "Patient"				# specifies that the source is defined for the Patient geometry
sc:So/XCAT_yellow_bone_marrow_activitySource/ActiveMaterial = "XCAT_yellow_bone_marrow_activity"			# specifies which material within the Patient geometry is active i.e. contain radionuclides
s:So/XCAT_yellow_bone_marrow_activitySource/BeamParticle    = "e+"					# tells which particle will be distributed throughout the active material
ic:So/XCAT_yellow_bone_marrow_activitySource/NumberOfHistoriesInRun = 1				# the number of particles that will be randomly distributed throughout the active material
b:So/XCAT_yellow_bone_marrow_activitySource/RecursivelyIncludeChildren = "True"			
ic:So/XCAT_yellow_bone_marrow_activitySource/MaxNumberOfPointsToSample = 10000			

#defines the energy spectrum of the particles in the source
s:So/XCAT_yellow_bone_marrow_activitySource/BeamEnergySpectrumType          = "Discrete"
dv:So/XCAT_yellow_bone_marrow_activitySource/BeamEnergySpectrumValues       = 1 0 MeV		# currently set to 0 MeV so all the particles are at rest to begin
uv:So/XCAT_yellow_bone_marrow_activitySource/BeamEnergySpectrumWeights      = 1 1.0		# defines the percentages of the particles at the respective energy value

